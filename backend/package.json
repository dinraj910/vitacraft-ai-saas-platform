    "multi-tenant",
    "resume-generator"
  ],
  "author": "DINRAJ K DINESH",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.1000.0",
    "@aws-sdk/s3-request-presigner": "^3.1000.0",
